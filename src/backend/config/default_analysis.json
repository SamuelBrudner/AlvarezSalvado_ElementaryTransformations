    "resource_utilization": {
      "memory_tracking": true,
      "cpu_utilization_monitoring": true,
      "processing_time_analysis": true,
      "throughput_calculation": true,
      "scalability_assessment": true
    }
  },
  "statistical_comparison": {
    "significance_level": 0.05,
    "multiple_comparison_correction": "benjamini_hochberg",
    "effect_size_calculation": true,
    "bootstrap_iterations": 10000,
    "correlation_methods": [
      "pearson",
      "spearman"
    ],
    "normality_tests": [
      "shapiro_wilk",
      "kolmogorov_smirnov"
    ],
    "hypothesis_testing": {
      "parametric_tests": [
        "t_test",
        "anova"
      ],
      "non_parametric_tests": [
        "mann_whitney_u",
        "kruskal_wallis",
        "wilcoxon_signed_rank"
      ],
      "equivalence_testing": true,
      "power_analysis": true
